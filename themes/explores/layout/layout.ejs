<html>
<head>
	<meta charset="UTF-8">
	<!-- Use <%= page.title %> for dynamic title -->
	<title><%= config.title %></title>
	<%- css(['https://fonts.googleapis.com/css?family=Lato|Quicksand:300|Ubuntu+Mono', 'https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css', 'css/style']) %>

</head>
<body>

	<%- partial('_partial/header', null, {cache: !config.relative_link}) %>

	<%- body %>
	
	<%- partial('_partial/footer', null, {cache: !config.relative_link}) %>
	
	<%- js(['https://code.jquery.com/jquery-3.1.1.min.js', 'js/script']) %>
	<% if(is_home()) { %>
		<%- js(['js/parallax', 'js/home']) %>
	<% } %>
</body>
</html>